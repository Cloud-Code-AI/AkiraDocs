{
  "name": "akiradocs-types",
  "version": "1.0.1",
  "description": "Type definitions for AkiraDocs",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["dist/*"]
    }
  },
  "files": [
    "dist/**",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "typescript",
    "types",
    "akiradocs"
  ],
  "author": "Cloud Code AI Inc",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}